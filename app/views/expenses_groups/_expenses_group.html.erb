<div id="<%= dom_id expenses_group %>" class="group-item">
  
  <div class="group-item-name-date">
  <strong>Transaction : <%= expenses_group.expense.name.capitalize %></strong>
  <p>
    Date:
    <%= expenses_group.created_at.strftime("%d-%m-%Y") %> 
    at 
    <%= expenses_group.created_at.strftime("%I:%M%p") %>
    </p>
  </div>

  <p class="group-item-sum">$ <%= expenses_group.amount %></p>
  

</div>